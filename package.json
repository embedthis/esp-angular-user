{
    "name": "esp-angular-user",
    "description": "User authentication and authorization for ESP",
    "version": "5.5.0",
    "keywords": [
        "esp"
    ],
    "dependencies": {
        "esp-angular-skeleton": "~5.5"
    },
    "devDependencies": {
        "pak": ">=0.11.3"
    },
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-angular-user.git"
    },
    "pak": {
        "export": {
            "from": [
                "contents/**",
                "controllers/*.c",
                "db/**",
                "migrations/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        },
        "manage": "esp.json"
    }
}
